{
	"compilerOptions": {
		"resolveJsonModule": true,

		"module": "commonjs",
		"target": "ES2020",
		"lib": ["ES2020"],
		"sourceMap": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"noImplicitThis": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
